{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "properties": {
        "title": {
            "title": "The title Schema",
            "type": "string",
            "default": "",
            "examples": [
                "carts"
            ]
        },
        "salary": {
            "title": "The salary Schema",
            "type": "integer",
            "default": 0,
            "examples": [
                100
            ]
        },
        "equity": {
            "title": "The equity Schema",
            "type": "number",
            "minimum": 0,
            "maximum": 1,
            "default": 0.0,
            "examples": [
                0.3
            ]
        }
    },
    "examples": [{
        "title": "carts",
        "salary": 100,
        "equity": 0.3,
        "company_handle": "c1"
    }]
}