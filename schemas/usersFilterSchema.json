{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "username",
        "id"
    ],
    "properties": {
        "username": {
            "title": "The username Schema",
            "type": "string",
            "default": "",
            "examples": [
                "hello"
            ]
        },
        "id": {
            "title": "The id Schema",
            "type": "integer",
            "default": 0,
            "examples": [
                3
            ]
        }
    },
    "examples": [{
        "username": "hello",
        "id": 3
    }]
}